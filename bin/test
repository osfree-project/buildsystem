#!/usr/local/bin/regina
#
#

args = ' -f   " /qwe/rrt/tyyy/yu.op   /qwe/rgfrgr/gtrgtg/rtgrt.u "'

s = 'q'
do while s \= ''
  s = getarg()
  say s
end


exit 0
/* ====================================================== */
getarg: procedure expose args

/* Gets one word, or a line, enclosed
   in quotes, from args               */

args = strip(args)

if pos('"', args) == 1 then
  parse value args with '"' opt '"' args
else
  parse var args opt args


return opt
/* ====================================================== */
